{
  "title": "Drug Discovery Kernel Observability",
  "panels": [
    {
      "type": "graph",
      "title": "Kernel p95 Latency",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(drug_discovery_request_latency_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Kernel Throughput",
      "targets": [
        {
          "expr": "sum(rate(drug_discovery_requests_total{endpoint='/drug_discovery/kernel'}[5m]))"
        }
      ]
    }
  ]
}
