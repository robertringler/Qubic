{
  "title": "Supply Chain Kernel Observability",
  "panels": [
    {
      "type": "graph",
      "title": "Kernel p95 Latency",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(supply_chain_request_latency_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Kernel Throughput",
      "targets": [
        {
          "expr": "sum(rate(supply_chain_requests_total{endpoint='/supply_chain/kernel'}[5m]))"
        }
      ]
    }
  ]
}
