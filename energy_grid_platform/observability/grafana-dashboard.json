{
  "title": "Energy Grid Kernel Observability",
  "panels": [
    {
      "type": "graph",
      "title": "Kernel p95 Latency",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(energy_grid_request_latency_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Kernel Throughput",
      "targets": [
        {
          "expr": "sum(rate(energy_grid_requests_total{endpoint='/energy_grid/kernel'}[5m]))"
        }
      ]
    }
  ]
}
